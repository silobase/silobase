GET /rest/v1/posts?group_by=user_id&having.likes.gt.20
GET /rest/v1/posts?join=users:on=posts.user_id=users.id&select=posts.id,users.name,posts.title,posts.likes&where=likes.gt.10
GET /rest/v1/posts?join=users:on=posts.user_id=users.id&select=users.country,COUNT(posts.id) as total_posts&group_by=users.country
